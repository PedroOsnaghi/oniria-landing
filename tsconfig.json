{
  "extends": "astro/tsconfigs/strict",
  "files": [
    ".astro/types.d.ts"
  ],
  "include": [
    ".astro/types.d.ts",
    "src/**/*",
    "src/types/**/*.d.ts"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["@types/three"],
    "moduleResolution": "bundler"
  }
}